{
  "name": "sheettemplate",
  "version": "2.0.0",
  "description": "Character sheet development in VS Code",
  "main": "index.js",
  "devDependencies": {
    "@types/underscore": "^1.13.0",
    "chokidar-cli": "^3.0.0",
    "jstransformer-uglify-es": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "pug": "3.0.3",
    "pug-cli": "1.0.0-alpha6",
    "sass": "1.97.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "jq": "^1.7.2"
  },
  "scripts": {
    "dev": "run-p build:pug:watch build:sass:watch watch:html",
    "build:pug": "pug -o build/ src/sheet.pug",
    "build:pug:watch": "pug -w -o build/ src/sheet.pug",
    "build:sass": "sass --no-source-map --style compressed --load-path=src/scss src/styles.scss ./build/styles.css",
    "build:sass:watch": "sass --no-source-map --style compressed --load-path=src/scss src/styles.scss ./build/styles.css --watch",
    "build:translations": "bash ./scripts/extract_i18n.sh",
    "purge:css": "bash ./scripts/purge_build_css.sh",
    "watch:html": "chokidar \"build/sheet.html\" -c \"npm run post:html\" --debounce 300",
    "post:html": "npm run build:translations && npm run purge:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clevett/roll20-SheetTemplate.git"
  },
  "keywords": [
    "Roll20"
  ],
  "author": "Caden Levett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clevett/SheetTemplate/issues"
  },
  "homepage": "https://github.com/clevett/SheetTemplate#readme"
}
