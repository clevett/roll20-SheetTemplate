{
  "name": "sheettemplate",
  "version": "2.0.0",
  "description": "Character sheet development in VS Code",
  "main": "index.js",
  "devDependencies": {
    "@types/underscore": "^1.13.0",
    "chokidar-cli": "^3.0.0",
    "jstransformer-uglify-es": "^0.2.1",
    "npm-run-all": "^4.1.5",
    "pug": "3.0.3",
    "pug-cli": "1.0.0-alpha6",
    "sass": "1.97.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "jq": "^1.7.2"
  },
  "scripts": {
    "dev": "tsc && run-p watch:ts watch:pug watch:sass watch:html",
    "build": "tsc && run-p build:pug && npm run build:sass && npm run build:translations",
    "build:pug": "pug -o build/ src/sheet.pug",
    "watch:pug": "pug -w -o build/ src/sheet.pug",
    "build:sass": "sass --no-source-map --style compressed --load-path=src/scss src/styles.scss ./build/styles.css",
    "watch:sass": "sass --no-source-map --style compressed --load-path=src/scss src/styles.scss ./build/styles.css --watch",
    "build:translations": "bash ./scripts/extract_i18n.sh",
    "watch:ts": "tsc -w -p tsconfig.json",
    "watch:html": "chokidar build/sheet.html -c \"npm run post:html\" --debounce 300",
    "post:html": "npm run build:translations"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clevett/roll20-SheetTemplate.git"
  },
  "keywords": [
    "Roll20"
  ],
  "author": "Caden Levett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clevett/SheetTemplate/issues"
  },
  "homepage": "https://github.com/clevett/SheetTemplate#readme"
}
